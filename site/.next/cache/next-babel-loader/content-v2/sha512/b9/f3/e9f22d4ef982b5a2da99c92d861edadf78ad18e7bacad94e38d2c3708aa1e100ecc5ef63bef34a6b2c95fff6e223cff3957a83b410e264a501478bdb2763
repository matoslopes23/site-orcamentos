 <FormGroup>\r\n              <Label for=\"whatsapp\">Whatsapp</Label>\r\n              <Input\r\n                type=\"text\"\r\n                name=\"whatsapp\"\r\n                id=\"iwhatsapp\"\r\n                placeholder=\"(XX)XXXX-XXXX\"\r\n                onChange={onChangInput}\r\n              />\r\n            </FormGroup>\r\n            \r\n            <FormGroup>\r\n              <Label for=\"projeto\">Projeto</Label>\r\n              <Input\r\n                type=\"textarea\"\r\n                name=\"projeto\"\r\n                id=\"iprojeto\"\r\n                placeholder=\"Fale um pouco sobre seu projeto\"\r\n                onChange={onChangInput}\r\n              />\r\n            </FormGroup>\r\n            {respose.formSave ? <Button type=\"submit\" outline color=\"danger\" disabled >Enviando...</Button>:<Button type=\"submit\" outline color=\"primary\">Solicitar orÃ§amento</Button>}\r\n            \r\n          </Form>\r\n        </Container>\r\n      </Jumbotron>\r\n      <Rodape/>\r\n    </div>\r\n    \r\n  );\r\n}\r\n\r\nexport default orcamento;\r\n"]},"metadata":{},"sourceType":"module"}